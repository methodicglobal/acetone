{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Acetone": {
    "ClusterConnectionStrings": [
      "localhost:19000"
    ],
    "ApplicationNameLocation": "Subdomain",
    "CredentialsType": "Local",
    "PartitionCacheLimit": 100,
    "PartitionCacheTtlSeconds": 30,
    "EnableLogging": true,
    "RetryCount": 3,
    "CircuitBreakerThreshold": 5
  },
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "cluster1": {
        "Destinations": {
          "destination1": {
            "Address": "https://example.com/"
          }
        }
      }
    }
  }
}
